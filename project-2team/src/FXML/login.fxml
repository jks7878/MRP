<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="login" 
stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="Main.LoginController">
   <children>
      <TextField fx:id="txtId" layoutX="340.0" layoutY="140.0" prefHeight="30.0" prefWidth="220.0" promptText="아이디">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPw" layoutX="340.0" layoutY="185.0" prefHeight="30.0" prefWidth="220.0" promptText="비밀번호">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="340.0" layoutY="240.0" mnemonicParsing="false" onAction="#loginProc" prefHeight="38.0" prefWidth="220.0" style="-fx-background-color: #909090;" text="로그인" textFill="WHITE">
         <font>
            <Font name="SimSun" size="14.0" />
         </font></Button>
      <ImageView fitHeight="21.0" fitWidth="86.0" layoutX="475.0" layoutY="346.0" onMouseClicked="#joinForm" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/login/btn_join.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
